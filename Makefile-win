# Makefile for MaTest, dos/win* version

CC = gcc.exe
#SYSTEM = POSIX
SYSTEM = WIN
VERBOSITY = -ansi
CFLAGS = -O3 -pipe -ftracer -fexec-charset=UTF-8 -D$(SYSTEM)

OBJS = MaTest.o connectives.o wffs_pn.o variables.o user.o evaluation.o

all : MaTest

MaTest : $(OBJS)
	$(CC) $(OBJS) -o MaTest

$(OBJS) : MaTest.h
